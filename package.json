{
	"name": "obsidian-githobs",
	"version": "1.0.1",
	"description": "Use Obsidian as Github issue editor!",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && npm install && git add manifest.json versions.json package-lock.json",
		"format": "npx eslint . --ext .ts --fix && npx prettier . --write"
	},
	"keywords": [
		"github",
		"issue",
		"obsidian",
		"obsidian-plugin",
		"obsidian-md",
		"obsidian-vault"
	],
	"author": "GabAlpha and MarcoG",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
